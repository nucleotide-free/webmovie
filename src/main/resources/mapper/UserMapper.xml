<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.software.dao.UserEntityMapper">

    <!--1-->
    <select id="selectUserInfo" parameterType="map" resultType="java.lang.String">
        select userId
        from user
        WHERE userName = #{username,jdbcType=VARCHAR} and #{password,jdbcType=VARCHAR}
    </select>



</mapper>